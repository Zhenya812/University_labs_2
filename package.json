{
  "name": "university_labs_2",
  "version": "1.0.0",
  "private": true,
  "description": "Workspace configuration for ESLint and Prettier for University Labs 2",
  "repository": {
    "type": "git",
    "url": "https://github.com/Zhenya812/-University_labs_2.git"
  },
  "scripts": {
    "lint": "eslint \"**/*.js\" --ignore-path .eslintignore",
    "lint:fix": "eslint \"**/*.js\" --fix --ignore-path .eslintignore",
    "format": "prettier --write \"**/*.{js,md,json,css,mdx}\" --ignore-path .prettierignore",
    "format:check": "prettier --check \"**/*.{js,md,json,css,mdx}\" --ignore-path .prettierignore"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "prettier": "^2.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^4.0.0"
  },
  "author": "Zhenya812",
  "license": "MIT"
  }
